{
  "name": "@audc/botframework-directlinejs",
  "version": "0.15.9",
  "description": "Client library for the Microsoft Bot Framework Direct Line 3.0 protocol",
  "files": [
    "dist/**/*",
    "lib/**/*"
  ],
  "main": "lib/directLine.js",
  "types": "lib/directLine.d.ts",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "build": "npm run build:typecheck && npm run build:babel -- --env-name test && npm run build:webpack -- --config webpack-development.config.js",
    "build:babel": "babel --extensions .js,.ts --ignore src/**/*.spec.js,src/**/*.spec.ts,src/**/*.test.js,src/**/*.test.ts --out-dir lib src",
    "build:typecheck": "tsc",
    "build:webpack": "webpack",
    "clean": "rimraf dist lib",
    "prepublishOnly": "npm run build:typecheck && npm run build:babel && npm run build:webpack",
    "start": "npm run build && concurrently --names \"babel,typecheck,webpack\" \"npm run build:babel -- --watch\" \"npm run build:typecheck -- --preserveWatchOutput --watch\" \"npm run build:webpack -- --config webpack-watch.config.js --watch\"",
    "test": "jest --silent",
    "watch": "npm run start"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/audiocodes/BotFramework-DirectLineJS"
  },
  "author": "Microsoft Corporation",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.28.6",
    "botframework-streaming": "^4.23.3",
    "buffer": "^6.0.3",
    "core-js": "^3.48.0",
    "cross-fetch": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "rxjs": "^5.5.12",
    "url-search-params-polyfill": "^8.2.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.6",
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-typescript": "^7.28.5",
    "@testing-library/dom": "^10.4.1",
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.2.3",
    "@types/ws": "^8.18.1",
    "babel-jest": "^30.2.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-istanbul": "^7.0.1",
    "babel-plugin-transform-inline-environment-variables": "^0.4.4",
    "concurrently": "^9.2.1",
    "dotenv": "^17.3.1",
    "event-target-shim": "^6.0.2",
    "express": "^5.2.1",
    "get-port": "^7.1.0",
    "global-agent": "^4.1.2",
    "has-resolved": "^2.0.0",
    "http-proxy": "^1.18.1",
    "http-proxy-middleware": "^3.0.5",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "jsdom": "^28.1.0",
    "nock": "^14.0.11",
    "node-fetch": "^3.3.2",
    "on-error-resume-next": "^2.0.3",
    "p-defer": "^4.0.1",
    "path-to-regexp": "^8.3.0",
    "restify": "^11.1.0",
    "rimraf": "^6.1.2",
    "simple-update-in": "^2.2.0",
    "typescript": "^5.9.3",
    "webpack": "^5.105.2",
    "webpack-cli": "^6.0.1",
    "webpack-stats-plugin": "^1.1.3",
    "ws": "^8.19.0"
  }
}
